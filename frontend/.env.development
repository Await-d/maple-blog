# Maple Blog Frontend Development Environment Configuration

# API configuration
VITE_API_URL=http://localhost:5002
VITE_APP_NAME=Maple Blog
VITE_APP_VERSION=1.0.0
VITE_NODE_ENV=development

# WebSocket configuration
VITE_WEBSOCKET_URL=ws://localhost:5002

# Debug settings
VITE_ENABLE_DEBUG=true
VITE_ENABLE_ANALYTICS=false

# Default locale
VITE_DEFAULT_LOCALE=zh-CN
VITE_SUPPORTED_LOCALES=zh-CN,en-US

# API settings
VITE_API_TIMEOUT=30000
VITE_PAGINATION_PAGE_SIZE=20
VITE_SEARCH_DEBOUNCE_MS=300

# PWA settings
VITE_ENABLE_PWA=false
VITE_ENABLE_OFFLINE=false