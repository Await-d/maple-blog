# 开发环境配置
NODE_ENV=development
VITE_APP_TITLE=Maple Blog 管理后台
VITE_APP_VERSION=1.0.0

# API配置
VITE_API_BASE_URL=http://localhost:5000
VITE_API_PREFIX=/api

# 应用配置
VITE_APP_MOCK=false
VITE_APP_ENABLE_DEVTOOLS=true
VITE_APP_ENABLE_ANALYTICS=false

# 安全配置
VITE_APP_CSP_ENABLED=true
VITE_APP_HTTPS_ONLY=false

# 缓存配置
VITE_APP_CACHE_ENABLED=true
VITE_APP_CACHE_TIMEOUT=300000

# 日志配置
VITE_APP_LOG_LEVEL=debug
VITE_APP_LOG_ENABLED=true