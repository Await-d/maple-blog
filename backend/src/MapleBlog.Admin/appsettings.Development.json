{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information",
      "Microsoft.EntityFrameworkCore": "Information"
    }
  },
  "AdminSecurity": {
    "RequireHttps": false,
    "AllowedIpRanges": [
      "127.0.0.1",
      "::1",
      "192.168.0.0/16",
      "10.0.0.0/8"
    ]
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug"
    }
  }
}